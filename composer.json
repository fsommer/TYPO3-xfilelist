{
    "name": "jokumer/xfilelist",
    "type": "typo3-cms-extension",
    "description": "Extends TYPO3 filelist and filebrowser for search within metadata, adds pagination and sorting to filebrowser",
    "keywords": [
        "TYPO3",
        "extension"
    ],
    "authors": [
        {
            "name": "J.Kummer",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/jokumer/TYPO3-xfilelist",
    "license": "GPL-3.0",
    "require": {
        "typo3/cms-core": "^10"
    },
    "autoload": {
        "psr-4": {
            "Jokumer\\Xfilelist\\": "Classes/"
        }
    },
    "replace": {
        "xfilelist": "self.version",
        "typo3-ter/xfilelist": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "xfilelist"
        }
    }
}